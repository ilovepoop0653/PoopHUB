local orion = loadstring(game:HttpGet("https://raw.githubusercontent.com/jadpy/suki/refs/heads/main/orion"))()
local W = orion:MakeWindow({
    Name = "poophub.ui",
    HidePremium = false,
    SaveConfig = false,
    ConfigFolder = "poophub_ui"
})
local g = W:MakeTab({
    Name = "poophub",
   Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})


g:AddButton({
Name = "PoophubES",
    Callback = function()
        (loadstring(game:HttpGet("https://raw.githubusercontent.com/jadpy/poophub/refs/heads/main/poophubsub")))()

orion:Destroy()
    end
})
g:AddButton({
    Name = "PoophubJP",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/jadpy/poophub/refs/heads/main/PoophubJP"))()

orion:Destroy()
    end
})
orion:Init()